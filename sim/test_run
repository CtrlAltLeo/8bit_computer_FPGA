#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x65438e795ff0 .scope module, "test" "test" 2 1;
 .timescale 0 0;
v0x65438e7a88f0_0 .var "clk", 0 0;
v0x65438e7a89c0_0 .net "count_out", 3 0, v0x65438e7a86f0_0;  1 drivers
v0x65438e7a8a90_0 .var "reset", 0 0;
S_0x65438e796180 .scope module, "c1" "program_counter" 2 21, 3 2 0, S_0x65438e795ff0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /OUTPUT 4 "count";
v0x65438e75c490_0 .net "clk", 0 0, v0x65438e7a88f0_0;  1 drivers
v0x65438e7a86f0_0 .var "count", 3 0;
v0x65438e7a87d0_0 .net "reset", 0 0, v0x65438e7a8a90_0;  1 drivers
E_0x65438e75cb00 .event posedge, v0x65438e7a87d0_0, v0x65438e75c490_0;
    .scope S_0x65438e796180;
T_0 ;
    %wait E_0x65438e75cb00;
    %load/vec4 v0x65438e7a87d0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x65438e7a86f0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x65438e7a86f0_0;
    %cmpi/e 15, 0, 4;
    %jmp/0xz  T_0.2, 4;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x65438e7a86f0_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x65438e7a86f0_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x65438e7a86f0_0, 0;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x65438e795ff0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x65438e7a8a90_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x65438e7a88f0_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x65438e795ff0;
T_2 ;
    %delay 0, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x65438e7a8a90_0, 0, 1;
    %delay 1, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x65438e7a8a90_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x65438e795ff0;
T_3 ;
    %delay 10, 0;
    %load/vec4 v0x65438e7a88f0_0;
    %nor/r;
    %store/vec4 v0x65438e7a88f0_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x65438e795ff0;
T_4 ;
    %delay 200, 0;
    %vpi_call 2 18 "$finish" {0 0 0};
    %end;
    .thread T_4;
    .scope S_0x65438e795ff0;
T_5 ;
    %vpi_call 2 25 "$monitor", "Time:\011%t\011Counter: %b", $time, v0x65438e7a89c0_0 {0 0 0};
    %end;
    .thread T_5;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "8bit_bench.v";
    "program_counter.v";
